                    <% if (feedData && feedData.catalog && feedData.catalog.sections) { %>
                        <% let counter = 2; %>
                        <% Object.values(feedData.catalog.sections).forEach((section) => { %>
                            <% if ((section.id >= 5 && section.id <= 14) || section.id == 220) { %>
                                <li class="header-nav__submenu-item header-nav__submenu-item--level-1 text-default">
                                    <a href="/products/<%= section.code %>/"
                                       class="header-nav__submenu-link header-nav__submenu-link--level-1"
                                       data-submenu-id="<%= counter %>">
                                        <%= section.name %>
                                    </a>
                                </li>
                                <% counter++; %>
                            <% } %>
                        <% }); %>
                    <% } %>
                </ul>
                <%
                const sectionIds = ["15", "105", "164", "166", "171", "221", "173", "168", "174", "175", "176"];
                %>
                <% sectionIds.forEach((sectionId, index) => { %>
                    <ul class="header-nav__submenu header-nav__submenu--level-2 text-lead" data-id="<%= index + 2 %>"
                        data-animate="">
                        <% if (feedData && feedData.catalog && feedData.catalog.sections) { %>
                            <%
                                const section = feedData.catalog.sections[sectionId];
                                const sectionCode = section ? section.code : '';
                            %>
                            <% if (feedData.catalog.elements) { %>
                                <%
                                    const elements = Object.values(feedData.catalog.elements).filter(element => element.section_id === sectionId);
                                %>
                                <% if (elements.length > 0) { %>
                                    <% elements.forEach((element) => { %>
                                        <li class="header-nav__submenu-item header-nav__submenu-item--level-2">
                                            <a href="/products/<%= sectionCode %>/<%= element.code %>/"
                                               class="header-nav__submenu-link header-nav__submenu-link--level-2">
                                                <%= element.name %>
                                            </a>
                                        </li>
                                    <% }); %>
                                <% } %>
                            <% } %>
                        <% } %>
