                        data-animate="">
                        <% if (feedData && feedData.catalog && feedData.catalog.sections) { %>
                            <%
                                const section = feedData.catalog.sections[sectionId];
                                const sectionCode = section ? section.code : '';
                            %>
                            <% if (feedData.catalog.elements) { %>
                                <%
                                    const elements = Object.values(feedData.catalog.elements).filter(element => element.section_id === sectionId);
                                %>
                                <% if (elements.length > 0) { %>
                                    <% elements.forEach((element) => { %>
