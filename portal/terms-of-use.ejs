                    <% if (feedData && feedData.catalog && feedData.catalog.sections) { %>
                        <% let counter = 2; %>
                        <%
                            // Получаем массив секций и сортируем его по полю sort
                            const sortedSections = Object.values(feedData.catalog.sections).sort((a, b) => a.sort - b.sort);
                        %>
                        <% sortedSections.forEach((section) => { %>
                            <% if ((section.id >= 5 && section.id <= 14) || section.id == 220) { %>
                                <li class="header-nav__submenu-item header-nav__submenu-item--level-1 text-default">
                                    <a href="/products/<%= section.code %>/"
                                       class="header-nav__submenu-link header-nav__submenu-link--level-1"
                                       data-submenu-id="<%= counter %>">
                                        <%= section.name %>
                                    </a>
                                </li>
                                <% counter++; %>
                            <% } %>
                        <% }); %>
                    <% } %>
